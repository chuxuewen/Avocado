server
	temp				out/temp
	port 				1026
	auto_index 	true
	#gzip				false
	#agzip				false
	#print_log	 	true
	defaults		index.html index.htm default.html
	max_file_size 				1073741824 # 1024 * 1024 * 1024
	max_form_data_size		52428800 # 50 * 1024 * 1024
	max_upload_file_size 	52428800 # 50 * 1024 * 1024
	router
		,
			match			/Tools/{action}/{args}
			service		Tools
		,
			match			/{path}
			service		StaticFile
			action		unknown
